<?php

namespace App\Menu;

use Sebastienheyd\Boilerplate\Menu\Builder;

class {{NAME}}
{
    public function make(Builder $menu)
    {
        $menu->add('{{NAME}}', ['route' => 'boilerplate.home', 'permission' => 'backend', 'icon' => 'square'])
             ->id('{{NAME}}')
             ->activeIfRoute('boilerplate.*')
             ->order(100);
    }
}
